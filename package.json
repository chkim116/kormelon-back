{
	"name": "kormelon-blog-server",
	"version": "1.0.0",
	"description": "Kormelon Blog 서버",
	"main": "./dist/index.js",
	"scripts": {
		"test": "jest --forceExit --detectOpenHandles",
		"test:c": "jest --forceExit --detectOpenHandles --coverage",
		"build": "rm -rf dist && tsc",
		"dev": "nodemon",
		"prestart": "npm run build",
		"start": "NODE_ENV=production node ./dist/index.js",
		"typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js -f src/ormConfig.ts",
		"migration:create": "typeorm migration:create -c production -d src/model/migration -n migration",
		"migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -c production",
		"migration:revert": "typeorm migration:revert -c production"
	},
	"dependencies": {
		"aws-sdk": "2.1073.0",
		"bcrypt": "5.0.1",
		"cookie-parser": "1.4.6",
		"cors": "2.8.5",
		"dayjs": "1.10.7",
		"express": "4.17.2",
		"express-validator": "6.14.0",
		"helmet": "5.0.2",
		"jsonwebtoken": "8.5.1",
		"morgan": "1.10.0",
		"multer": "1.4.4",
		"multer-s3": "2.10.0",
		"mysql2": "2.3.3",
		"node-schedule": "2.1.0",
		"reading-time": "1.5.0",
		"reflect-metadata": "0.1.13",
		"request-ip": "2.1.3",
		"swagger-jsdoc": "6.1.0",
		"swagger-ui-express": "4.3.0",
		"typeorm": "0.2.41",
		"winston": "3.5.1",
		"winston-daily-rotate-file": "4.6.0"
	},
	"devDependencies": {
		"@types/bcrypt": "5.0.0",
		"@types/cookie-parser": "1.4.2",
		"@types/cors": "2.8.12",
		"@types/express": "4.17.13",
		"@types/helmet": "4.0.0",
		"@types/jest": "27.4.0",
		"@types/jsonwebtoken": "8.5.8",
		"@types/morgan": "1.9.3",
		"@types/multer": "1.4.7",
		"@types/multer-s3": "2.7.11",
		"@types/node": "17.0.13",
		"@types/node-schedule": "1.3.2",
		"@types/request-ip": "0.0.37",
		"@types/supertest": "2.0.11",
		"dotenv": "15.0.0",
		"jest": "27.4.7",
		"nodemon": "2.0.15",
		"supertest": "6.2.2",
		"ts-jest": "27.1.3",
		"ts-node": "10.5.0",
		"typescript": "4.5.5"
	},
	"keywords": [
		"블로그"
	],
	"author": "kimchanghoe <cskim132@gmail.com>",
	"license": "MIT"
}
